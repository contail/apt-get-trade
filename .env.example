# 한국투자증권 API 인증 정보
APP_KEY=발급받은_APP_KEY
APP_SECRET=발급받은_APP_SECRET
ACCOUNT_NO=810XXXXX  # 종합계좌번호 앞 8자리
PRODUCT_CODE=01      # 계좌상품코드 뒤 2자리
BASE_URL=https://openapi.koreainvestment.com:9443

# 데이터베이스 설정
DATABASE_URL=postgresql://user:password@localhost:5432/apt_get_trade

# 로깅 설정
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# 애플리케이션 설정
DEBUG=True
SECRET_KEY=your_secret_key_here
ALLOWED_HOSTS=localhost,127.0.0.1

# 데이터베이스 설정
DB_ENGINE=django.db.backends.postgresql
DB_NAME=apt_get_trade_db
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432

# Redis 설정
REDIS_URL=redis://localhost:6379/0

# 해외주식 API 설정
STOCK_API_KEY=your_api_key_here
STOCK_API_SECRET=your_api_secret_here
STOCK_API_BASE_URL=https://api.example.com/v1

# 계좌 API 설정
ACCOUNT_API_KEY=your_account_api_key_here
ACCOUNT_API_SECRET=your_account_api_secret_here
ACCOUNT_API_BASE_URL=https://api.example.com/account/v1

# 이메일 설정
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_HOST_USER=your_email@gmail.com
EMAIL_HOST_PASSWORD=your_app_password
EMAIL_USE_TLS=True 